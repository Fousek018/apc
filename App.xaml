<Application x:Class="LABPOWER_APC.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:LABPOWER_APC"
             xmlns:View="clr-namespace:LABPOWER_APC.View"
             xmlns:Utilities="clr-namespace:LABPOWER_APC.Utilities"
             xmlns:ViewModel="clr-namespace:LABPOWER_APC.ViewModel"
             xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
             ShutdownMode="OnExplicitShutdown">
    <Application.Resources>
        <ResourceDictionary>
        <DataTemplate x:Name="UPSVIEW_Templates" DataType="{x:Type ViewModel:UPS}">
            <View:UPSVIEW/>
        </DataTemplate>



        <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/MaterialDesignThemes.Wpf;component/Themes/MaterialDesign2.Defaults.xaml" />
                <ResourceDictionary Source="Animations.xaml" />
                <materialDesign:BundledTheme BaseTheme="Light" PrimaryColor="Blue" SecondaryColor="LightGreen" />


            </ResourceDictionary.MergedDictionaries>
            
            
        </ResourceDictionary>

    </Application.Resources>
</Application>
